{
	"name" : "romano83/cakephp3-media",
	"description" : "Media plugin for CakePHP 3",
	"type" : "cakephp-plugin",
	"require" : {
		"php" : ">=5.4.0"
	},
	"require-dev" : {
		"cakephp/cakephp" : "~3.0",
		"phpunit/phpunit" : "*",
		"cakephp/cakephp-codesniffer" : "2.*"
	},
	"suggest" : {
		"ezyang/htmlpurifier" : "dev-master"
	},
	"autoload" : {
		"psr-4" : {
			"Romano83\\Cakephp3Media\\" : "src"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Romano83\\Cakephp3Media\\Test\\" : "tests",
			"Romano83\\Cakephp3Media\\Test\\Fixture\\" : "tests/Fixture",
			"Cake\\Test\\" : "./vendor/cakephp/cakephp/tests"
		}
	},
	"authors" : [{
			"name" : "Romain Guillemin",
			"email" : "contact@romain-guillemin.fr",
			"homepage" : "http://www.romain-guillemin.fr",
			"role" : "Developper & Maintainer"
		}, {
			"name" : "Jonathan Boyer",
			"email" : "contact@grafikart.fr",
			"homepage" : "http://jonathan-boyer.fr/",
			"role" : "Initial Author"
		}
	],
	"keywords" : [
		"Plugin",
		"Media",
		"CakePHP"
	],
	"support" : {
		"source" : "https://github.com/Romano83/CakePHP3-Media",
		"issues" : "https://github.com/Romano83/CakePHP3-Media/issues"
	}
}